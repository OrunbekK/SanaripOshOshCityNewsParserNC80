{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug"
  },
  "ParserSettings": {
    "HttpTimeoutSeconds": 60,
    "RateLimitRequestsPerSecond": 5,
    "RetryPolicyAttempts": 5,
    "RetryDelayMilliseconds": 500
  }
}